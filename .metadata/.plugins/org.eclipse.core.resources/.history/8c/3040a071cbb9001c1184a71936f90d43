package Package_Program;

public class TestaWhile {
	
	public static void main(String[] args) {
		int contador = 0;
		int total = 0;
		while (contador <= 10) {
			total += contador;
			System.out.println(total);
			contador++;
		}
		
		
		
		for(int cont = 0; cont <=10; cont++  ) {
			System.out.println(cont);
		}
	}

}
